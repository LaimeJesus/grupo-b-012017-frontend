        <meta name="viewport" content="width=device-width, initial-scale=1">
  </head>
  <base href="/">
  <body data-ng-app="aloloco-app">
    <!--  class="navbar navbar-default navbar-fixed-top"-->
    <nav class="navbar navbar-default" ng-controller="MainController">
      <div class="container-fluid">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="/">A lo loco</a>
          <spinner
                 name="generalSpinner"
                 show="false"
                 img-src="../images/ajax-loader.gif">
          </spinner>
        </div>
        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
          <ul class="nav navbar-nav">
            <form class="navbar-form navbar-left">
              <div class="form-group">
                <input type="text" class="form-control" placeholder="Search">
              </div>
              <button type="submit" class="btn btn-default">Submit</button>
            </form>
          </ul>
          <ul class="nav navbar-nav navbar-right">
            <li><a href="/">{{'navbar.products' | translate}}</a></li>
            <li><a href="offers">{{'navbar.offers' | translate}}</a></li>
            <li data-ng-show="someoneLogged()"><a href="createOffer">{{'navbar.createoffer' | translate}}</a></li>
            <li data-ng-hide="someoneLogged()"><a href="login">{{'navbar.login' | translate}}</a></li>
            <li data-ng-hide="someoneLogged()"><a href="signup">{{'navbar.signup' | translate}}</a></li>
            <li class="dropdown" data-ng-show="someoneLogged()">
              <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Dropdown <span class="caret"></span></a>
              <ul class="dropdown-menu">
                <li data-ng-show="someoneLogged()"><a href="myprofile">{{'navbar.profile' | translate}}</a></li>
                <li data-ng-show="someoneLogged()"><a href="mylists">{{'navbar.mylists' | translate}}</a></li>
                <li role="separator" class="divider"></li>
                <li ng-click="logout()" data-ng-show="someoneLogged()"><a href="#">{{'navbar.logout' | translate}}</a></li>
              </ul>
            </li>
          </ul>
        </div>
      </div>
    </nav>
    <hr />

    <div ng-view></div>

    <script src="app/app.js"></script>
    <script src="app/services/services.js"></script>
    <script src="app/services/offer_service.js"></script>
    <script src="app/services/product_list_service.js"></script>
    <script src="app/services/product_service.js"></script>
    <script src="app/services/shop_service.js"></script>
    <script src="app/services/user_service.js"></script>
    <script src="app/controllers/controllers.js"></script>
    <script src="app/controllers/main_controller.js"></script>
    <script src="app/controllers/product_controller.js"></script>
    <script src="app/controllers/product_list_controller.js"></script>
    <script src="app/controllers/delivery_controller.js"></script>
    <script src="app/controllers/home_offer_controller.js"></script>
    <script src="app/controllers/login_controller.js"></script>
    <script src="app/controllers/sign_up_controller.js"></script>
    <script src="app/controllers/profile_controller.js"></script>
